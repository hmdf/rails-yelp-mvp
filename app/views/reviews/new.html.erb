<h1>New Reviews</h1>

<%= simple_form_for [@restaurant, @review] do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :rating %>
    <%= f.input :content %>
  </div>

  <%= f.submit "Submit", class: "btn btn-primary" %>
  <%= link_to 'Back', restaurant_path(@restaurant), class: "btn btn-primary"  %>


<% end %>



